---
layout: none
---
[
  {% for post in site.posts %}
    {
      "title"      : {{ post.title | jsonify }},
      "subtitle"   : {{ post.subtitle | jsonify }},
      "categories" : "{{ post.categories | join: ', ' }}",
      "tags"       : "{{ post.tags | join: ', ' }}",
      "url"        : "{{ post.url | prepend: site.baseurl }}",
      "date"       : "{{ post.date | date: '%Y.%m.%d' }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]